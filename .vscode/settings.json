{
    "files.associations": {
        "*.plist": "xml",
        "*.uxl": "xml"
    },
    "files.exclude": {
        "**/.uno": true,
        "**/.vs": true,
        "**/bin": true,
        "**/build": true,
        "**/node_modules": true,
        "**/packages": true,
        "**/obj": true,
        "**/*.user": true,
        "*.exe": true,
        "*.pkg": true,
        "*.tgz": true
    },
    "cmake.configureOnOpen": false,
    "dotnet.defaultSolution": "disable"
}